// fnWsUsers â€” thin wrapper for /workspaces/{id}/users
// Returns: table with {user: record, workspace_role: text}  (raw CoLabEndpoint output)
// Inherits: pagination, retry (FetchWithRetry), cycle guard from CoLabEndpoint
let
  fnWsUsers = (wid as text) as table =>
    CoLabEndpoint("workspaces/" & wid & "/users", false, null, [page_size = "100"])
in
  fnWsUsers
